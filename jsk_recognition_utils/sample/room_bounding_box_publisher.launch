<launch>
  <node name="room_bounding_box_publisher"
        pkg="jsk_recognition_utils" type="bounding_box_array_publisher.py">
    <rosparam>
      frame_id: map
      rate: 30
      boxes:
      - position: [0.755, -0.501, 1.500]
        dimension: [4.0, 4.0, 2.5]
        rotation: [0.025, 0.000, 0.419]
      - position: [-0.171, 3.410, 1.500]
        dimension: [5.2, 3.5, 2.5]
        rotation: [0.025, 0.000, 0.419]
      - position: [3.414, 5.096, 1.500]
        dimension: [3.8, 2.0, 2.5]
        rotation: [0.025, 0.000, 0.419]
      - position: [6.288, 5.874, 1.500]
        dimension: [3.5, 3.8, 2.5]
        rotation: [0.025, 0.000, 0.419]
      - position: [8.016, 2.535, 1.500]
        dimension: [4.0, 4.0, 2.5]
        rotation: [0.025, 0.000, 0.419]
    </rosparam>
  </node>

  <node name="marker_cube1"
        pkg="jsk_interactive_marker" type="marker_6dof"
        output="screen">
    <rosparam>
      frame_id: /interactive1
      object_type: line
      object_r: 1.0
      object_g: 0.2
      object_b: 0.2
      object_a: 1.0
      publish_tf: true
      tf_frame: hoge1
    </rosparam>
  </node>

  <node name="marker_cube2"
        pkg="jsk_interactive_marker" type="marker_6dof">
    <rosparam>
      frame_id: /map
      object_type: line
      object_r: 1.0
      object_g: 0.2
      object_b: 0.2
      object_a: 1.0
      publish_tf: true
      tf_frame: interactive2
    </rosparam>
  </node>
</launch>

